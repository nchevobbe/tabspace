{

  "manifest_version": 2,
  "name": "TabSpace",
  "version": "0.1",

  "description": "TabGroup/Panorama like.",

  "icons": {
    "48": "icons/tabspace.svg"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/tabspace.svg",
    "default_title": "TabSpace"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      }
    }
  },

  "background": {
    "scripts": [
      "src/background-script.js"
    ]
  },

  "permissions": [
    "tabs",
    "history"
  ],

  "web_accessible_resources": []

}
