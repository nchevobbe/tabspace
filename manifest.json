{
  "manifest_version": 2,
  "name": "TabSpace",
  "version": "0.1",

  "description": "TabGroup/Panorama like.",

  "icons": {
    "48": "icons/tabspace.svg"
  },

  "browser_action": {
    "default_icon": "icons/tabspace.svg",
    "default_title": "TabSpace"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      }
    }
  },

  "background": {
    "scripts": ["dist/background.js"]
  },

  "permissions": ["activeTab", "history", "tabs"],

  "web_accessible_resources": []
}
